#tag Events Timer1
	#tag Event
		Sub Action()
		  If Proc.Suspended Then
		    Self.Title = Proc.Name + " (" + Str(Proc.ProcessID) + ") " + " (Suspended)"
		    PushButton2.Caption = "Resume"
		  Else
		    Self.Title = Proc.Name + " (" + Str(Proc.ProcessID) + ") "
		    PushButton2.Caption = "Suspend"
		  End If
		End Sub
	#tag EndEvent
