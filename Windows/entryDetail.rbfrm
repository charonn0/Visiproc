#tag WindowCode
	#tag Event
		Sub Open()
		  PushButton1.SetFocus()
		  Me.Top = Window1.Top + 50
		  Me.Left = Window1.Left + 115
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h0
		Sub showMe(process As ProcessInformation)
		  Proc = process
		  Me.Title = Proc.Name + " (" + Str(Proc.ProcessID) + ") "
		  If Proc.Suspended Then
		    PushButton2.Caption = "Resume"
		    Me.Title = Me.Title + " (Suspended)"
		  Else
		    PushButton2.Caption = "Suspend"
		  End If
		  If proc.Name = "svchost.exe" Or proc.Name = "Services.exe" Then
		    If proc.Modules.Ubound = -1 Then proc.LoadModules
		    If proc.Modules.Ubound > -1 Then 
		      Self.Width = 736
		      Listbox1.InitialValue = "Modules"
		      For Each T As ModuleInformation In proc.Modules
		        Listbox1.AddRow(T.Path)
		      Next
		    Else
		      Self.Width = 394
		    End If
		  Else
		    Call GetWindowListForProcess(process)
		    If process.Windows.Ubound > -1 Then
		      Self.Width = 736
		      For Each win As ProcWindow In process.Windows
		        Listbox1.AddRow(win.Title)
		        Listbox1.RowTag(Listbox1.LastIndex) = win
		      Next
		    Else
		      Self.Width = 394
		    End If
		  End If
		  productNaMe.Text = process.Name
		  PID.Text = Str(process.ProcessID)
		  priority.Text = Str(process.BaseThreadPriority)
		  threads.Text = Str(process.NumberOfThreads)
		  
		  Try
		    Dim currentProcs() As ProcessInformation = GetActiveProcesses
		    For i As Integer = 0 to UBound(currentProcs)
		      If currentProcs(i).ProcessID = process.ParentProcessID then
		        parent.Text = currentProcs(i).Name + " (" + Str(process.ParentProcessID) + ")"
		        Exit For
		      End If
		    next
		  End Try
		  
		  If process.path <> Nil Then
		    path.Text = process.path.AbsolutePath
		    Dim verInfo As New VersionInformation(process.path)
		    If verInfo <> Nil then
		      company.Text = verInfo.CompanyName
		      copyright.Text = verInfo.LegalCopyright
		      description.Text = verInfo.FileDescription
		      productNaMe.Text = verInfo.ProductName
		      trademark.Text = verInfo.LegalTrademarks
		      version.Text = verInfo.FileVersion
		    End If
		  End If
		  procicon = process.largeIcon 
		  cmdLine.Text = process.CommandLine
		  
		  Self.Show
		  
		Exception Err
		  
		  
		End Sub
	#tag EndMethod


	#tag Property, Flags = &h0
		Proc As ProcessInformation
	#tag EndProperty

	#tag Property, Flags = &h0
		procicon As Picture
	#tag EndProperty


